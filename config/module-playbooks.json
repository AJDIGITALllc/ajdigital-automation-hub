{
  "playbooks": [
    {
      "id": "client-delivery",
      "objective": "Manages end-to-end client project delivery, from onboarding through completion, ensuring consistent service quality and timely deliverables",
      "kpis": [
        {
          "name": "Project completion rate within timeline",
          "target": "95",
          "unit": "percent"
        },
        {
          "name": "Client satisfaction scores",
          "target": "4.5",
          "unit": "score_out_of_5"
        },
        {
          "name": "Resource utilization efficiency",
          "target": "85",
          "unit": "percent"
        },
        {
          "name": "Budget adherence",
          "target": "95",
          "unit": "percent"
        },
        {
          "name": "Post-delivery client retention rate",
          "target": "90",
          "unit": "percent"
        }
      ],
      "tools": [
        {
          "name": "Whop",
          "role": "Payment processing and customer management",
          "category": "payments"
        },
        {
          "name": "n8n",
          "role": "Workflow automation and orchestration",
          "category": "automation"
        },
        {
          "name": "GBU",
          "role": "Contract generation and management",
          "category": "legal"
        },
        {
          "name": "MailerLite",
          "role": "Client communication and onboarding sequences",
          "category": "email"
        }
      ],
      "workflow_steps": [
        {
          "id": "onboarding",
          "label": "Client Onboarding",
          "description": "Collect requirements and gather client information"
        },
        {
          "id": "planning",
          "label": "Project Planning",
          "description": "Set up milestones and schedule deliverables"
        },
        {
          "id": "allocation",
          "label": "Resource Allocation",
          "description": "Assign team members and manage tasks"
        },
        {
          "id": "tracking",
          "label": "Progress Tracking",
          "description": "Monitor status updates and provide reporting"
        },
        {
          "id": "delivery",
          "label": "Delivery & Handoff",
          "description": "Conduct quality assurance and obtain client approval"
        }
      ],
      "client_inputs": [
        "Project requirements and objectives",
        "Budget and timeline constraints",
        "Brand assets and guidelines",
        "Access credentials to relevant platforms",
        "Communication preferences"
      ],
      "internal_checks": [
        "Contract signed and payment received",
        "Resource availability confirmed",
        "Milestone deadlines tracked",
        "Quality assurance completed",
        "Client feedback collected"
      ],
      "automations": [
        "Whop payment → n8n → create/send GBU contract",
        "Contract signed → n8n → send onboarding/welcome via MailerLite",
        "Project complete → n8n → send review/referral sequence"
      ],
      "maintenance_cadence": {
        "daily": ["Project status updates", "Resource monitoring"],
        "weekly": ["Client check-ins", "Milestone reviews"],
        "biweekly": ["Resource allocation optimization"],
        "monthly": ["Client satisfaction surveys", "Process improvements"],
        "quarterly": ["Full delivery pipeline analysis", "Methodology updates"]
      }
    },
